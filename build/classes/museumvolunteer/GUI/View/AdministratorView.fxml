                <TableColumn fx:id="nameAdminColumn" prefWidth="150.0" text="Navne" />
            </columns>
        </TableView>
        <TableView fx:id="hoursAdminTable" layoutX="554.0" layoutY="130.0" prefHeight="200.0">
            <columns>
            <TableColumn fx:id="dateAdminColumn" prefWidth="150.0" text="Dato" />
                <TableColumn fx:id="hoursAdminColumn" prefWidth="150.0" text="Timer" />
            </columns>
        </TableView>
        <Button layoutX="32.0" layoutY="23.0" mnemonicParsing="false" onAction="#addGuildButton" styleClass="buttonGreen" text="Tilføj Laug">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button layoutX="199.0" layoutY="23.0" mnemonicParsing="false" onAction="#addVolunteerButton" styleClass="buttonGreen" text="Tilføj Frivillig">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button layoutX="37.0" layoutY="77.0" mnemonicParsing="false" onAction="#removeGuildButton" styleClass="buttonGreen" text="Slet Laug">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button layoutX="204.0" layoutY="77.0" mnemonicParsing="false" onAction="#removeVolunteersButton" styleClass="buttonGreen" text="Slet Frivillig">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button layoutX="370.0" layoutY="23.0" mnemonicParsing="false" onAction="#addManagerButton" styleClass="buttonGreen" text="Tilføj Manager">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button layoutX="375.0" layoutY="77.0" mnemonicParsing="false" onAction="#removeManagerButton" styleClass="buttonGreen" text="Slet Manager">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <TableView fx:id="managerAdminTable" layoutX="373.0" layoutY="130.0" prefHeight="200.0" prefWidth="150.0">
            <columns>
                <TableColumn fx:id="managerAdminColumn" prefWidth="150.0" text="Manager" />
            </columns>
        </TableView>
        <Button layoutX="571.0" layoutY="77.0" mnemonicParsing="false" onAction="#removeHoursButton" styleClass="buttonGreen" text="Slet Timer">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button layoutX="566.0" layoutY="23.0" mnemonicParsing="false" onAction="#addHoursButton" styleClass="buttonGreen" text="Tilføj Timer">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
        <Button layoutX="611.0" layoutY="350.0" mnemonicParsing="false" onAction="#returnAdmin" styleClass="buttonGreen" text="Tilbage">
            <font>
                <Font size="20.0" />
            </font>
        </Button>
      <TextField fx:id="txtFieldHours" layoutX="693.0" layoutY="77.0" prefHeight="31.0" prefWidth="146.0" />
      <DatePicker fx:id="datePicker" layoutX="712.0" layoutY="18.0" prefHeight="31.0" prefWidth="163.0" />
      <Button fx:id="exportToExcel" layoutX="734.0" layoutY="357.0" mnemonicParsing="false" onAction="#handleExportToExcel" styleClass="buttonGreen" text="Button" />
