<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="ManagerScreen" prefHeight="380.0" prefWidth="727.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="museumvolunteer.GUI.Controller.ManagerViewController">
   <children>
      <TableView fx:id="guildManagerTable" layoutX="14.0" layoutY="115.0" onMouseClicked="#handleGuildsVolunteers" prefHeight="200.0" prefWidth="150.0">
        <columns>
          <TableColumn fx:id="guildManagerColumn" prefWidth="150.0" text="Laug" />
        </columns>
      </TableView>
      <TableView fx:id="nameManagerTable" layoutX="183.0" layoutY="115.0" onMouseClicked="#handleVolunteersHours" prefHeight="200.0" prefWidth="150.0">
        <columns>
          <TableColumn fx:id="nameManagerColumn" prefWidth="150.0" text="Navne" />
        </columns>
      </TableView>
      <TableView fx:id="hoursManagerTable" layoutX="353.0" layoutY="115.0" prefHeight="200.0">
        <columns>
            <TableColumn prefWidth="75.0" text="Dato" />
          <TableColumn fx:id="hoursManagerColumn" prefWidth="150.0" text="Timer" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="185.0" layoutY="14.0" mnemonicParsing="false" onAction="#addVolunteersButton" text="Tilføj frivillige">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button layoutX="190.0" layoutY="60.0" mnemonicParsing="false" onAction="#deleteVolunteersButton" text="Slet frivillige">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button layoutX="365.0" layoutY="14.0" mnemonicParsing="false" onAction="#addHoursButton" text="Tilføj Timer">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button layoutX="370.0" layoutY="61.0" mnemonicParsing="false" onAction="#deleteHoursButton" text="Slet Timer">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button layoutX="410.0" layoutY="322.0" mnemonicParsing="false" onAction="#returnManager" text="Tilbage">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <DatePicker fx:id="datePicker" layoutX="537.0" layoutY="14.0" />
      <TextField fx:id="txtFieldHours" layoutX="550.0" layoutY="58.0" />
   </children>
</AnchorPane>
